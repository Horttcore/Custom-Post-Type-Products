    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Horttcore\\CustomPostTypeProducts\\": "src/"
        }
    },
    "require": {
        "horttcore/wp-custom-post-type": "^1.0.0",
        "horttcore/wp-custom-taxonomy": "^1.0.0",
        "horttcore/wp-plugin": "^1.0.0"
    }
