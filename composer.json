{
    "name": "horttcore/custom-post-type-products",
    "description": "A WordPress custom post type for managing products",
    "type": "wordpress-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Ralf Hortt",
            "email": "me@horttcore.de"
        }
    ],
    "repositories": [
